{
	"name": "obsidian-handlebars-dynamic",
	"version": "1.1.2",
	"description": "Handlebars dynamic templating for Obsidian",
	"main": "main.js",
	"scripts": {
		"dev": "cross-env DEBUG=1 node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && cross-env DEBUG=0 node esbuild.config.mjs production",
		"build-debug": "tsc -noEmit -skipLibCheck && cross-env DEBUG=1 node esbuild.config.mjs production",
		"lint": "eslint .",
		"lint:fix": "eslint . --fix",
		"version": "node version-bump.mjs && git add manifest.json versions.json"
	},
	"keywords": [],
	"author": "",
	"license": "MIT",
	"devDependencies": {
		"@types/node": "^24.10.9",
		"@typescript-eslint/eslint-plugin": "^8.53.1",
		"@typescript-eslint/parser": "^8.53.1",
	"cross-env": "^10.1.0",
		"esbuild": "^0.25.12",
		"eslint": "^9.39.2",
		"eslint-plugin-obsidianmd": "^0.1.9",
		"obsidian": "latest",
		"tslib": "^2.8.1",
		"typescript": "^5.9.3"
	},
	"dependencies": {
		"handlebars": "^4.7.8",
		"yaml": "^2.8.2"
	}
}
